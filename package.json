  "scripts": {
    "serve:docker": "docker-compose --env-file .env.example -f docker-compose.yml -f docker-compose.safe.yml up --build -d",
    "serve:contracts": "nx serve contracts",
    "serve:frontend": "nx run-many --target=serve --projects=dashboard,public",
    "serve:backend": "yarn serve:docker && NODE_TLS_REJECT_UNAUTHORIZED='0' nx run-many --target=serve --projects=api,auth",
    "serve:api": "NODE_TLS_REJECT_UNAUTHORIZED='0' nx serve api",
    "serve:auth": "NODE_TLS_REJECT_UNAUTHORIZED='0' nx serve auth",
    "ngrok": "~/ngrok http --domain=local.auth.thx.network https://localhost:3030  > /dev/null &",
    "safe:install": "docker compose exec txs-web python manage.py insert_safe_master_copy --address \"0xC44951780f195Ed71145e3d0d2F25726A097C348\"",
    "import-quests": "yarn build api; cp -R ./dist/libs ./dist/apps/api/libs; yarn nx run api:import-quests",
    "script": "NODE_TLS_REJECT_UNAUTHORIZED='0' nx run api:script",
    "build": "nx build",
    "test": "NODE_OPTIONS='--max-old-space-size=8192' nx test"
  },
