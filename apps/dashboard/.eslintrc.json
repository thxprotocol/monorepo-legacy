{
  "extends": [
    "../../.eslintrc.json",
    "plugin:vue/essential",
    "@vue/typescript/recommended",
    "prettier"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@nrwl/nx/enforce-module-boundaries": "off"
  },
  "ignorePatterns": ["!**/*"],
  "env": {
    "node": true
  },
  "overrides": [
    {
      "files": ["**/*.spec.{j,t}s?(x)"],
      "env": {
        "jest": true
      }
    }
  ]
}
